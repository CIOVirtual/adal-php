{
    "name": "saintsystems/adal-php",
    "description": "Microsoft Azure Active Directory Authentication Library (ADAL) for PHP",
    "keywords": ["azure", "ad", "active directory", "authentication"],
    "license": "MIT",
    "type": "project",
    "authors": [
        {
            "name": "Saint Systems",
            "email": "contact@saintsystems.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "guzzlehttp/guzzle": "^6.1",
        "monolog/monolog": "^1.22"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.0"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "ADAL\\": "src/"
        }
    },
    "config": {
        "preferred-install": "dist"
    }
}
